{
  "expo": {
    "name": "tezDealz-MobileApp",
    "slug": "tezDealz-MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/tezDealz.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.text.tezDealz",
      "config":{
        "googleSignIn":{
          "reservedClientId":"com.googleusercontent.apps.202707505931-el11o7903mrqmg8p8abnmm6hda4sda52"
        }
      },
      "googleServicesFile":"./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "package": "com.test.tezDealz",
      "googleServicesFile":"./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
